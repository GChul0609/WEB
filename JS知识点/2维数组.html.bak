	var arr=[];
    var arr1=[1,2,3,4,5];
	var arr2=[6,7,8,9,0];
	arr.myConcat=function(){	
		for(var i=0;i<arr1.length;i++){
			arr[this.length]=arr1[i];
		}
		for(var j=0;j<arr2.length;j++){
			arr[this.length]=arr2[j];
		}
